{
  "name": "react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "cz": "yarn --cwd .. commit",
    "commit": "yarn --cwd .. commit",
    "dev": "esno server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && yarn build:server && esno prerender",
    "serve": "cross-env NODE_ENV=production esno server",
    "lint:tsc": "tsc",
    "lint:script": "eslint --ext .tsx --ignore-path .gitignore ."
  },
  "dependencies": {
    "i18next": "^19.9.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.8.9",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esno": "^0.4.6",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.5",
    "vite-plugin-windicss": "^0.8.2"
  }
}
